<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Video Dashboard</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
  <link href="https://unpkg.com/tailwindcss@^2/dist/tailwind.min.css" rel="stylesheet">
</head>
<body class="bg-gray-100 font-sans text-gray-900">
  <div class="flex h-screen">
    <nav class="w-64 bg-white shadow-md h-full flex flex-col">
        <div class="p-4">
          <h1 class="text-2xl font-bold mb-2 truncate">Chapters</h1>
          <ul class="space-y-4">
            <li><a href="/" class="text-gray-700 hover:text-blue-500"><i class="fas fa-home"></i> Home</a></li>
            <li><a href="/upload" class="text-gray-700 hover:text-blue-500"><i class="fas fa-upload"></i> Upload Video</a></li>
            <li><a href="/" class="text-gray-700 hover:text-blue-500"><i class="fas fa-user"></i> Profile</a></li>
            <li><a href="#" class="text-gray-700 hover:text-blue-500"><i class="fas fa-cog"></i> Settings</a></li>
          </ul>
        </div>
      </nav>

    <main class="flex-grow overflow-y-auto px-4 py-8">
      <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-4">
       
      </div>
    </main>
  </div>
  <script>
    const menuButton = document.getElementById('menuButton');
    const menuLinks = document.getElementById('menuLinks');

    menuButton.addEventListener('click', () => {
      menuLinks.classList.toggle('hidden');
    });
  </script>
</body>
</html>
